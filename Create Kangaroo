#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
    int x1,v1,x2,v2,i;
    scanf("%d %d %d %d",&x1,&v1,&x2,&v2);
    for(i=0;i<10000;i++)
    {
        x1=x1+v1;
        x2=x2+v2;
        if(x1==x2)
        break;
    }
    if(x1==x2)
    printf("YES");
    else
    printf("NO");
    return 0;
}
